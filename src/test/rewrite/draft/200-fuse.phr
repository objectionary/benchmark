---
# yamllint disable rule:line-length
name: fuse-map-map
pattern: |
  ⟦
    𝐵0,
    𝜏-class ↦ ⟦
      φ ↦ Φ.jeo.class,
      𝐵-class-head,
      𝜏-method ↦ ⟦
        φ ↦ Φ.jeo.method,
        𝐵-method-head,
        body ↦ ⟦
          𝐵-body-head,
          𝜏-first ↦ ⟦
            φ ↦ Φ.hone.reduceX,
            method ↦ 𝑒-first-method-name,
            in ↦ 𝑒-first-input-type,
            body ↦ ⟦ 𝐵-first-body ⟧
          ⟧,
          𝜏-second ↦ ⟦
            φ ↦ Φ.hone.reduceX,
            method ↦ 𝑒-second-method-name,
            in ↦ 𝑒-second-input-type,
            body ↦ ⟦ 𝐵-second-body ⟧
          ⟧,
          𝐵-body-tail
        ⟧,
        𝐵-method-tail
      ⟧,
      𝐵-class-tail,
      𝐵6
    ⟧,
    𝐵7
  ⟧
result: |
  ⟦
    𝐵0,
    𝜏-class ↦ ⟦
      φ ↦ Φ.jeo.class,
      𝐵-class-head,
      𝜏-method ↦ ⟦
        φ ↦ Φ.jeo.method,
        𝐵-method-head,
        body ↦ ⟦
          𝐵-body-head,
          𝜏-first ↦ ⟦
            φ ↦ Φ.hone.reduceX,
            method ↦ 𝑒-first-method-name,
            in ↦ 𝑒-first-input-type,
            body ↦ ⟦
              𝐵-first-body,
              𝐵-second-body
            ⟧
          ⟧,
          𝐵-body-tail
        ⟧,
        𝐵-method-tail
      ⟧,
      𝐵-class-tail,
      𝐵6
    ⟧,
    𝐵7
  ⟧
where:
  - meta: 𝜏-fuse
    function: random-tau
    args: ['base', '𝐵1', '𝐵6', '𝜏-method']
  - meta: 𝜏-fuse-opcode
    function: random-tau
    args: ['𝐵3', '𝐵4']
  - meta: 𝑒-fuse-method-name
    function: concat
    args:
      - 𝑒-first-method-name
      - '"_fuse"'
  - meta: 𝑒-fuse-descriptor
    function: concat
    args:
      - '"("'
      - 𝑒-first-input-type
      - '"Ljava/util/function/Consumer;)V"'
  - meta: 𝑒-first-descriptor
    function: concat
    args:
      - '"("'
      - 𝑒-first-input-type
      - '")"'
      - 𝑒-first-output-type
  - meta: 𝑒-second-descriptor
    function: concat
    args:
      - '"("'
      - 𝑒-second-input-type
      - '")"'
      - 𝑒-second-output-type
