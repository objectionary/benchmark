---
# yamllint disable rule:line-length
name: dup-before-filter-distill
pattern: |
  ⟦
    φ ↦ Φ.hone.distill,
    class ↦ 𝑒-class,
    bridge-input ↦ 𝑒-bridge-input,
    bridge-output ↦ 𝑒-bridge-output,
    start ↦ "filter",
    accepted ↦ 𝑒-accepted,
    returned ↦ 𝑒-returned,
    body ↦ ⟦ 𝐵-body, ρ ↦ ∅ ⟧
  ⟧
result: |
  ⟦
    φ ↦ Φ.hone.distill,
    class ↦ 𝑒-class,
    bridge-input ↦ 𝑒-bridge-input,
    bridge-output ↦ 𝑒-bridge-output,
    start ↦ "none",
    accepted ↦ 𝑒-accepted,
    returned ↦ 𝑒-returned,
    body ↦ ⟦
      i-first-dup ↦ ⟦ φ ↦ Φ.jeo.opcode.dup ⟧,
      𝐵-body,
      ρ ↦ ∅
    ⟧
  ⟧
