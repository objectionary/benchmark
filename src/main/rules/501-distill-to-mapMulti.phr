---
# yamllint disable rule:line-length
name: distill-to-mapMulti
pattern: |
  ⟦
    φ ↦ Φ.jeo.class,
    𝐵-class-head,
    𝜏-method ↦ ⟦
      φ ↦ Φ.jeo.method,
      𝐵-method-head,
      body ↦ ⟦
        𝐵-body-head,
        𝜏-distill ↦ ⟦
          φ ↦ Φ.hone.distill,
          class ↦ 𝑒-class,
          target-class ↦ 𝑒-target-class,
          target-input ↦ 𝑒-target-input,
          target-output ↦ 𝑒-target-output,
          bridge-input ↦ 𝑒-bridge-input,
          bridge-output ↦ 𝑒-bridge-output,
          locals ↦ ⟦ 𝐵-locals, ρ ↦ ∅ ⟧,
          body ↦ ⟦ 𝐵-body, ρ ↦ ∅ ⟧
        ⟧,
        𝐵-body-tail
      ⟧,
      𝐵-method-tail
    ⟧,
    𝐵-class-tail
  ⟧
result: |
  ⟦
    φ ↦ Φ.jeo.class,
    𝐵-class-head,
    𝜏-method ↦ ⟦
      φ ↦ Φ.jeo.method,
      𝐵-method-head,
      body ↦ ⟦
        𝐵-body-head,
        𝜏-distill ↦ ⟦
          φ ↦ Φ.hone.mapMulti,
          class ↦ 𝑒-class,
          target-class ↦ 𝑒-target-class,
          target-method ↦ 𝑒-target-method,
          target-input ↦ 𝑒-target-input,
          bridge-input ↦ 𝑒-bridge-input
        ⟧,
        𝐵-body-tail
      ⟧,
      𝐵-method-tail
    ⟧,
    𝐵-class-tail,
    𝜏-reduce ↦ ⟦
      φ ↦ Φ.jeo.method,
      access ↦ 4106,
      descriptor ↦ 𝑒-signature,
      signature ↦ "",
      maxs ↦ ⟦
        φ ↦ Φ.jeo.maxs,
        max-stack ↦ 3,
        max-locals ↦ 𝑒-max-locals
      ⟧,
      params ↦ ⟦
        φ ↦ Φ.jeo.params,
        i1 ↦ ⟦
          φ ↦ Φ.jeo.param,
          index ↦ 0,
          access ↦ 0,
          type ↦ 𝑒-input-param
        ⟧,
        i2 ↦ ⟦
          φ ↦ Φ.jeo.param,
          index ↦ 1,
          access ↦ 0,
          type ↦ "Ljava/util/function/Consumer;"
        ⟧
      ⟧,
      body ↦ ⟦
        φ ↦ Φ.jeo.seq.of,
        i-pop-consumer ↦ ⟦
          φ ↦ Φ.jeo.opcode.aload,
          i1 ↦ 1
        ⟧,
        i-pop-item ↦ ⟦
          φ ↦ Φ.jeo.opcode.aload,
          i1 ↦ 0
        ⟧,
        𝐵-body,
        i-frame ↦ ⟦
          φ ↦ Φ.jeo.frame,
          type ↦ 0,
          nlocal ↦ 𝑒-max-locals,
          locals ↦ ⟦
            φ ↦ Φ.jeo.seq.of,
            𝐵-locals
          ⟧,
          nstack ↦ 2,
          stack ↦ ⟦
            φ ↦ Φ.jeo.seq.of2,
            consumer ↦ "java/util/function/Consumer",
            item ↦ 𝑒-bridge-output
          ⟧
        ⟧,
        i-invoke ↦ ⟦
          φ ↦ Φ.jeo.opcode.invokeinterface,
          i2 ↦ "java/util/function/Consumer",
          i3 ↦ "accept",
          i4 ↦ "(Ljava/lang/Object;)V",
          i5 ↦ Φ̇.true
        ⟧,
        i-return ↦ ⟦
          φ ↦ Φ.jeo.opcode.return
        ⟧,
        ρ ↦ ∅
      ⟧,
      name ↦ 𝑒-target-method
    ⟧
  ⟧
where:
  - meta: 𝑒-input-param
    function: concat
    args:
      - '"L"'
      - 𝑒-bridge-input
      - '";"'
  - meta: 𝜏-reduce
    function: random-tau
    args: ['name', '𝐵-class-head', '𝐵-class-tail']
  - meta: 𝑒-target-method
    function: random-string
    args: [ '"reduce_%d"' ]
  - meta: 𝑒-signature
    function: concat
    args:
      - '"("'
      - 𝑒-input-param
      - '"Ljava/util/function/Consumer;)V"'
  - meta: 𝑒-max-locals
    function: size
    args: [ 𝐵-locals ]
