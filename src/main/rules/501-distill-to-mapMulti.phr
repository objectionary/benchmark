---
# yamllint disable rule:line-length
name: distill-to-mapMulti
pattern: |
  ⟦
    φ ↦ Φ.jeo.class,
    𝐵-class-head,
    𝜏-method ↦ ⟦
      φ ↦ Φ.jeo.method,
      𝐵-method-head,
      body ↦ ⟦
        𝐵-body-head,
        𝜏-distill ↦ ⟦
          φ ↦ Φ.hone.distill,
          class ↦ 𝑒-class,
          bridge-input ↦ 𝑒-bridge-input,
          bridge-output ↦ 𝑒-bridge-output,
          accepted ↦ 𝑒-accepted,
          returned ↦ 𝑒-returned,
          body ↦ ⟦ 𝐵-body, ρ ↦ ∅ ⟧
        ⟧,
        𝐵-body-tail
      ⟧,
      𝐵-method-tail
    ⟧,
    𝐵-class-tail
  ⟧
result: |
  ⟦
    φ ↦ Φ.jeo.class,
    𝐵-class-head,
    𝜏-method ↦ ⟦
      φ ↦ Φ.jeo.method,
      𝐵-method-head,
      body ↦ ⟦
        𝐵-body-head,
        𝜏-distill ↦ ⟦
          φ ↦ Φ.hone.mapMulti,
          class ↦ 𝑒-class,
          target-method ↦ 𝑒-target-method,
          bridge-input ↦ 𝑒-bridge-input
        ⟧,
        𝐵-body-tail
      ⟧,
      𝐵-method-tail
    ⟧,
    𝐵-class-tail,
    𝜏-distill-lambda ↦ ⟦
      φ ↦ Φ.jeo.method,
      access ↦ 4106,
      descriptor ↦ 𝑒-signature,
      signature ↦ "",
      maxs ↦ ⟦
        φ ↦ Φ.jeo.maxs,
        max-stack ↦ 4,
        max-locals ↦ 2
      ⟧,
      params ↦ ⟦
        φ ↦ Φ.jeo.params,
        i1 ↦ ⟦
          φ ↦ Φ.jeo.param,
          index ↦ 0,
          access ↦ 0,
          type ↦ 𝑒-bridge-input
        ⟧,
        i2 ↦ ⟦
          φ ↦ Φ.jeo.param,
          index ↦ 1,
          access ↦ 0,
          type ↦ "Ljava/util/function/Consumer;"
        ⟧
      ⟧,
      body ↦ ⟦
        φ ↦ Φ.jeo.seq.of,
        i-pop-item ↦ ⟦
          φ ↦ Φ.jeo.opcode.aload,
          i1 ↦ 0
        ⟧,
        𝐵-body,
        i-pop-consumer ↦ ⟦
          φ ↦ Φ.jeo.opcode.aload,
          i1 ↦ 1
        ⟧,
        i-swap ↦ ⟦ φ ↦ Φ.jeo.opcode.swap ⟧,
        i-invoke ↦ ⟦
          φ ↦ Φ.jeo.opcode.invokeinterface,
          i2 ↦ "java/util/function/Consumer",
          i3 ↦ "accept",
          i4 ↦ "(Ljava/lang/Object;)V",
          i5 ↦ Φ̇.true
        ⟧,
        i-return ↦ ⟦
          φ ↦ Φ.jeo.opcode.return
        ⟧,
        ρ ↦ ∅
      ⟧,
      name ↦ 𝑒-target-method
    ⟧
  ⟧
where:
  - meta: 𝜏-distill-lambda
    function: random-tau
    args: ['name', '𝐵-class-head', '𝐵-class-tail']
  - meta: 𝑒-target-method
    function: random-string
    args: [ '"distill_%d"' ]
  - meta: 𝑒-signature
    function: concat
    args:
      - '"("'
      - 𝑒-bridge-input
      - '"Ljava/util/function/Consumer;)V"'
